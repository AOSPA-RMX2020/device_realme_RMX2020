# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#TODO: Add 'check' as fs_mgr_flags with data partition.
# Currently we dont have e2fsck compiled. So fs check would failed.


#<src>                                                  <mnt_point>             <type>   <mnt_flags and options>                                                  <fs_mgr_flags> 
system                                                  /system                 ext4     ro                                                                       wait,,avb=vbmeta_system,logical,first_stage_mount,avb_keys=/avb/q-gsi.avbpubkey:/avb/r-gsi.avbpubkey:/avb/s-gsi.avbpubkey
vendor                                                  /vendor                 ext4     ro                                                                       wait,,avb,logical,first_stage_mount,async_discard
product                                                 /product                ext4     ro                                                                       wait,logical,first_stage_mount
system_ext                                              /system_ext             ext4     ro                                                                       wait,logical,first_stage_mount
odm                                                     /odm                    ext4     ro                                                                       wait,logical,first_stage_mount
/dev/block/by-name/md_udc                               /metadata               ext4     noatime,nosuid,nodev,discard                                             wait,formattable,first_stage_mount
/dev/block/by-name/cache                                /cache                  ext4     noatime,nosuid,nodev,noauto_da_alloc,async_discard                       wait,check,formattable
/dev/block/by-name/userdata                             /data                   ext4     noatime,nosuid,nodev,noauto_da_alloc,async_discard,errors=panic,inlinecrypt wait,check,formattable,quota,latemount,reservedsize=128m,resize,fileencryption=aes-256-xts:aes-256-cts:v1
/dev/block/by-name/userdata                             /data                   f2fs     noatime,nosuid,nodev,discard,noflush_merge,fsync_mode=nobarrier,reserve_root=134217,resgid=1065,inlinecrypt wait,check,formattable,quota,reservedsize=128m,latemount,resize,checkpoint=fs,fileencryption=aes-256-xts:aes-256-cts:v1
