<resources>

    <!-- Disable UI touch sounds by default -->
    <bool name="def_sound_effects_enabled">false</bool>

    <!-- Disable bluetooth by default -->
    <bool name="def_bluetooth_on">false</bool>

    <!-- Set the automatic brightness mode on by default -->
    <bool name="def_screen_brightness_automatic_mode">false</bool>

    <!-- Set Window animation scale to 50% by default -->
    <fraction name="def_window_animation_scale">50%</fraction>

    <!-- Set Window transition scale to 50% by default -->
    <fraction name="def_window_transition_scale">50%</fraction>

    <!-- Default for Settings.Global.DEVICE_NAME $1=MANUFACTURER $2=MODEL-->
    <string name="def_device_name">realme Monet</string>

    <!-- Default for Settings.Global.DEVICE_NAME $1=MODEL-->
    <string name="def_device_name_simple">realme Monet</string>

</resources>
